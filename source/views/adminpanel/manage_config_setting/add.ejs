<% layout('./../../layout/admin') -%>
<form name="form1" action="" method="POST" >
<div class="row">
    <div class="col-sm-4">
      <div class="card">
        <div class="card-header">
          <strong><%=adminController%></strong>
          <small><%=adminAction%></small>
        </div>
        <div class="card-body">      
          <div class="row">
            <div class="col-sm-12">
              <div class="form-group">
                <label for="key">Key</label>
                <input name="key" class="form-control" id="key" type="text" placeholder="Enter key" value="<%=helpers.helper.view_data(appPostParam.key)%>">                
              </div>
            </div>            
          </div>
          
          <div class="row">
            <div class="col-sm-12">
              <div class="form-group">
                <label for="value">Value</label>
                <input name="value" class="form-control" id="value" type="text" placeholder="Enter value" value="<%=helpers.helper.view_data(appPostParam.value)%>">                
              </div>
            </div>            
          </div>
          
          <!-- /.row-->
          <div class="row">
            <div class="col-sm-12">
              <div class="form-group">
                <button class="btn btn-sm btn-primary" type="submit">
                    <i class="fa fa-dot-circle-o"></i> Submit
                </button>
              </div>
            </div>
          </div>          
          </div>
          <!-- /.row-->
        </div>
      </div>
    </div>    
  </div>
</form>

<%-scriptjs%>