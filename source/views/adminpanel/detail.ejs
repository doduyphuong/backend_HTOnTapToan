<% layout('./../layout/admin') -%>
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <strong><%=adminController%>: </strong>
                <small><%=item._id%></small>
            </div>
            <div class="card-body">
                <div class="row" >
                    <div class="col-lg-12">
                        <!--group-->                                                
                        <a href="<%=returnUrl%>">Return</span></a>
                        <!--end group-->    
                    </div>
                </div>
                <% for(var i=0; i < out_fields.length; i++) { %>        
                    <div class="row" >
                        <div class="col-lg-12">                        
                            <% var t ='item.' + out_fields[i] %>
                            <b><%=out_fields[i]%> : </b> <%=eval(t)%>
                        </div>
                    </div>
                <% } %> 
            </div>
        </div> 
    </div>   
</div>